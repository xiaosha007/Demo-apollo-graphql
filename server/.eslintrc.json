{
    "env": {
        "browser": true,
        "es2021": true,
        "node":true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "linebreak-style": 0,
        "prettier/prettier": ["error", {
            "endOfLine":"auto"
          }]
    },
    "import/resolver": "webpack"
}
